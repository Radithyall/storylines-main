stock GetPlayerStreet(playerid, zone[], len = sizeof zone)
{
	new Float:x, Float:y, Float:z;
    GetPlayerPos(playerid, x, y, z);

 	for(new i = 0; i != sizeof(Streets); i++ )
 	{
		if(x >= Streets[i][STREET_MINX] && x <= Streets[i][STREET_MAXX] &&
		 	y >= Streets[i][STREET_MINY] && y <= Streets[i][STREET_MAXY])
		{
		    return format(zone, len, Streets[i][STREET_NAME]);
		}
	}
	return format(zone, len, "Unknown");
}

ReturnLocation(playerid)
{
	new zone[64], Float:x, Float:y, Float:z;

	GetPlayerPos(playerid, x, y, z);
	
	if(GetPlayerInterior(playerid))
	    strcat(zone, "Interior");
	else
	    strcat(zone, GetZoneName(x, y, z));

	return zone;
}

ReturnLocationStreet(playerid)
{
	new
		playerStreet[64],
		longStr[64];

	GetPlayerStreet(playerid, playerStreet);

	strcat(longStr, playerStreet);
	return longStr;
}

